<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btn_star"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginTop="24dp"
        android:text="즐겨찾기"
        android:textColor="#0B0A0A"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#00BCD4"
        app:layout_constraintEnd_toEndOf="@+id/btn_imformation"
        app:layout_constraintStart_toStartOf="@+id/btn_imformation"
        app:layout_constraintTop_toBottomOf="@+id/btn_imformation" />

    <Button
        android:id="@+id/btn_imformation"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="16dp"
        android:text="게시판"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#673AB7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_map"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <Button
        android:id="@+id/btn_onshop"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginTop="24dp"
        android:text="온라인샵"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#03A9F4"
        app:layout_constraintEnd_toEndOf="@+id/btn_map"
        app:layout_constraintStart_toStartOf="@+id/btn_map"
        app:layout_constraintTop_toBottomOf="@+id/btn_map" />

    <Button
        android:id="@+id/btn_recommand"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:text="추천알고리즘"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#2196F3"
        app:layout_constraintEnd_toEndOf="@+id/btn_offshop"
        app:layout_constraintStart_toStartOf="@+id/btn_offshop"
        app:layout_constraintTop_toBottomOf="@+id/btn_offshop" />

    <ImageView
        android:id="@+id/btn_home"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_baseline_home_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_onshop"
        app:layout_constraintStart_toStartOf="@+id/btn_recommand"
        app:layout_constraintTop_toBottomOf="@+id/btn_onshop" />

    <ImageView
        android:id="@+id/btn_menu"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="70dp"
        android:src="@drawable/ic_baseline_menu_64"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_home"
        app:layout_constraintTop_toBottomOf="@+id/btn_chatbot" />

    <ImageView
        android:id="@+id/btn_mypage"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="70dp"
        android:src="@drawable/ic_baseline_favorite_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_home"
        app:layout_constraintTop_toBottomOf="@+id/btn_star" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="178dp"
        android:layout_height="40dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="112dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/btn_search"
        android:layout_width="40dp"
        android:layout_height="36dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="15dp"
        android:src="@drawable/ic_baseline_search_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/logo"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_map"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp"
        android:text="지도찾기"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#9C27B0"
        app:layout_constraintEnd_toStartOf="@+id/btn_imformation"
        app:layout_constraintStart_toEndOf="@+id/btn_offshop"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <Button
        android:id="@+id/btn_chatbot"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginTop="24dp"
        android:text="챗봇"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#3F51B5"
        app:layout_constraintEnd_toEndOf="@+id/btn_recycle"
        app:layout_constraintStart_toStartOf="@+id/btn_recycle"
        app:layout_constraintTop_toBottomOf="@+id/btn_recycle" />

    <Button
        android:id="@+id/btn_recycle"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp"
        android:text="분리수거"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#F44336"
        app:layout_constraintEnd_toStartOf="@+id/btn_offshop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <Button
        android:id="@+id/btn_offshop"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="16dp"
        android:text="오프라인샵"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#E91E63"
        app:layout_constraintEnd_toStartOf="@+id/btn_map"
        app:layout_constraintStart_toEndOf="@+id/btn_recycle"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="414dp"
        android:layout_height="306dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo"
        app:srcCompat="@drawable/background" />


</androidx.constraintlayout.widget.ConstraintLayout>










