<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OnnShop">


    <Button
        android:id="@+id/btn_travel"
        android:layout_width="95dp"
        android:layout_height="69dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="40dp"
        android:background="#FFC107"
        android:text="TRAVEL"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_office" />

    <Button
        android:id="@+id/btn_office"
        android:layout_width="95dp"
        android:layout_height="69dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="29dp"
        android:background="#8BC34A"
        android:text="OFFICE"
        app:layout_constraintBottom_toTopOf="@+id/btn_travel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btn_bathroom"
        android:layout_width="95dp"
        android:layout_height="69dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="40dp"
        android:background="#FFEB3B"
        android:text="BATHROOM"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/btn_travel"
        app:layout_constraintStart_toEndOf="@+id/btn_living"
        app:layout_constraintTop_toBottomOf="@+id/btn_kitchen" />

    <Button
        android:id="@+id/btn_kitchen"
        android:layout_width="95dp"
        android:layout_height="69dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="29dp"
        android:background="#4CAF50"
        android:text="KITCHEN"
        app:layout_constraintBottom_toTopOf="@+id/btn_bathroom"
        app:layout_constraintEnd_toStartOf="@+id/btn_office"
        app:layout_constraintStart_toEndOf="@+id/btn_all"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btn_living"
        android:layout_width="95dp"
        android:layout_height="69dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="29dp"
        android:layout_marginBottom="40dp"
        android:background="#CDDC39"
        android:text="LIVING"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_all" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="248dp"
        android:layout_height="167dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="509dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/btn_all"
        android:layout_width="95dp"
        android:layout_height="69dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="29dp"
        android:background="#009688"
        android:text="All"
        app:layout_constraintBottom_toTopOf="@+id/btn_living"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="199dp"
        android:layout_height="40dp"
        android:layout_marginStart="106dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="106dp"
        android:text="장소에 따른 환경보호"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="40dp"
        android:src="@drawable/ic_baseline_home_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/btn_bathroom" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="40dp"
        android:src="@drawable/ic_baseline_favorite_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/btn_travel" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="40dp"
        android:src="@drawable/ic_baseline_menu_64"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_living" />
</androidx.constraintlayout.widget.ConstraintLayout>